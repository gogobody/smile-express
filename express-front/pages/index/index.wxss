.textarea{
  background:#fff;padding-left:15rpx;padding-right:0;height:300rpx;border-bottom:2rpx solid #e5e5e5
}
form .textarea textarea{
  width:100%;height:240rpx;padding-top:20rpx;color:#333;font-size:30rpx
}
.payblock{
  display:-webkit-box;padding:60rpx 0;box-sizing:border-box
