.tab{
  position:relative;background:#fff;margin:20rpx 0;height:156rpx;padding:0 30rpx
}
.column-list {
  display: flex;
  justify-content: space-between;
}
.column-list .column-item {
  flex: 1;
}
.tab:before{
  content:"";width:2rpx;position:absolute;background:#e5e5e5;left:50%;top:50%;height:100rpx;display:block;margin-left:-20rpx;margin-top:-60rpx
}
.slide:before{
  content:'';display:none}
.buy,.send{
  -webkit-box-flex:1;box-sizing:border-box
}
.buy text,.send text{
  margin-left:130rpx;display:block
}
.buy text.fz15,.send text.fz15{
  color:#333
}.buy text.fz13,.send text.fz13{
  color:#999
}
.addressInfo{
  background:#fcb900;font-size:32rpx;height:88rpx;color:#fff;line-height:88rpx
}
